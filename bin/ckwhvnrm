#!/bin/bash

MMDIR="/mnt/transcend/Pictures/Wallhaven"

[ -d ${MMDIR} ] || {
    MMDIR="/u/pictures/Wallhaven"
    [ -d ${MMDIR} ] || {
        echo "Cannot locate Wallhaven pics dir"
        exit 1
    }
}
cd ${MMDIR}

while read i
do
    [ -f $i ] && echo $i
done < /usr/local/MirrorCommand/etc/rmlist.txt
