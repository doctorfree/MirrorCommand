#!/bin/bash

MMDIR="/mnt/transcend/Pictures/MagicMirror"

[ -d ${MMDIR} ] || {
    MMDIR="/u/pictures/MagicMirror"
    [ -d ${MMDIR} ] || {
        echo "Cannot locate MagicMirror pics dir"
        exit 1
    }
}
cd ${MMDIR}

while read img
do
    rm -f $img
done < /usr/local/MirrorCommand/etc/rmlist.txt
